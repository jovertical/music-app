{
    "name": "palonponjovertlota/music-app",
    "type": "project",
    "description": "Simple music application",
    "keywords": [
        "music"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "fideloper/proxy": "^4.0",
        "inertiajs/inertia-laravel": "^0.2.4",
        "laravel/framework": "^6.2",
        "laravel/tinker": "^2.0",
        "spatie/laravel-medialibrary": "^7.18",
        "tightenco/ziggy": "^0.8.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "facade/ignition": "^1.4",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "nunomaduro/phpinsights": "^1.13",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "format": [
            "php-cs-fixer fix ."
        ],
        "test:insights": [
            "./vendor/bin/phpinsights analyse --config-path=config/insights.php --min-quality=85 --min-complexity=85 --min-architecture=85 --min-style=85 ."
        ],
        "test:unit": [
            "./vendor/bin/phpunit"
        ],
        "test": [
            "composer test:insights",
            "composer test:unit"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
